<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DetailsAct">

    <TextView
        android:id="@+id/website_details"
        android:layout_width="238dp"
        android:layout_height="42dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:gravity="center_vertical"
        android:autoLink="web"
        android:textColor="#FF0000"
        android:textColorLink="#FF0000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.352"
        app:layout_constraintStart_toEndOf="@+id/website"
        app:layout_constraintTop_toBottomOf="@+id/google_details"
        app:layout_constraintVertical_bias="0.02" />

    <TextView
        android:id="@+id/website"
        android:layout_width="95dp"
        android:layout_height="44dp"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="8dp"
        android:layout_weight="1"
        android:text="Website"
        android:gravity="center_vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/google"
        app:layout_constraintVertical_bias="0.012" />

    <TextView
        android:id="@+id/google_details"
        android:layout_width="238dp"
        android:layout_height="42dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:gravity="center_vertical"
        android:autoLink="web"
        android:textColor="#FF0000"
        android:textColorLink="#FF0000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.352"
        app:layout_constraintStart_toEndOf="@+id/google"
        app:layout_constraintTop_toBottomOf="@+id/rating_details" />

    <TextView
        android:id="@+id/google"
        android:layout_width="95dp"
        android:layout_height="44dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="8dp"
        android:layout_weight="1"
        android:text="Google Page"
        android:gravity="center_vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rating" />

    <RatingBar
        android:id="@+id/rating_details"
        android:layout_width="238dp"
        android:layout_height="42dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:progressTint="#FF0000"
        android:gravity="center_vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.352"
        app:layout_constraintStart_toEndOf="@+id/rating"
        app:layout_constraintTop_toBottomOf="@+id/price_details" />

    <TextView
        android:id="@+id/rating"
        android:layout_width="95dp"
        android:layout_height="44dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="8dp"
        android:layout_weight="1"
        android:text="Rating"
        android:gravity="center_vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/price" />

    <TextView
        android:id="@+id/price_details"
        android:layout_width="238dp"
        android:layout_height="42dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="12dp"
        android:text="TextView"
        android:gravity="center_vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.352"
        app:layout_constraintStart_toEndOf="@+id/price"
        app:layout_constraintTop_toBottomOf="@+id/phone_details" />

    <TextView
        android:id="@+id/price"
        android:layout_width="95dp"
        android:layout_height="44dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="8dp"
        android:layout_weight="1"
        android:text="Price Level"
        android:gravity="center_vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phone" />

    <TextView
        android:id="@+id/phone_details"
        android:layout_width="238dp"
        android:layout_height="42dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="12dp"
        android:text="TextView"
        android:gravity="center_vertical"
        android:autoLink="phone"
        android:textColor="#FF0000"
        android:textColorLink="#FF0000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.352"
        app:layout_constraintStart_toEndOf="@+id/phone"
        app:layout_constraintTop_toBottomOf="@+id/address_details" />

    <TextView
        android:id="@+id/phone"
        android:layout_width="95dp"
        android:layout_height="44dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="8dp"
        android:layout_weight="1"
        android:text="Phone Number"
        android:gravity="center_vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/address" />

    <TextView
        android:id="@+id/address"
        android:layout_width="95dp"
        android:layout_height="44dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="56dp"
        android:layout_weight="1"
        android:text="Address"
        android:gravity="center_vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/address_details"
        android:layout_width="238dp"
        android:layout_height="42dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="56dp"
        android:text="TextView"
        android:gravity="center_vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.352"
        app:layout_constraintStart_toEndOf="@+id/address"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttoninfo"
        android:drawableLeft="@drawable/info_outline"
        android:layout_width="103dp"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimary"
        android:text="INFO"
        android:onClick="info"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonphoto"
        android:drawableLeft="@drawable/photos"
        android:layout_width="101dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:background="@color/colorPrimary"
        android:text="PHOTOS"
        android:onClick="photo"
        app:layout_constraintStart_toStartOf="@+id/buttoninfo"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonmap"
        android:drawableLeft="@drawable/maps"
        android:layout_width="111dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="100dp"
        android:background="@color/colorPrimary"
        android:text="MAP"
        android:onClick="map"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonreview"
        android:drawableLeft="@drawable/review"
        android:layout_width="101dp"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimary"
        android:text="REVIEWS"
        android:onClick="reviews"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/twitter"
        android:layout_width="32dp"
        android:layout_height="24dp"
        android:drawableRight="@drawable/share"
        android:onClick="twitter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonreview" />

</android.support.constraint.ConstraintLayout>